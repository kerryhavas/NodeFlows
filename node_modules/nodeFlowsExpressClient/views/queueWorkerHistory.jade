
// woah!  I can use whack whack comments in jade
// also, more importantly, you can include things in the master page
block stylesheets
  link(rel='stylesheet', href='/stylesheets/chat.css')

block javascripts
 

section(class="main-content")

  h3= 'Queue Worker Details'
  div= 'Id: ' + queueWorker._id
  div= 'Type: ' + queueWorker.type
  div= 'Status: ' + queueWorker.status
  div= 'WorkflowId: ' + queueWorker.workflowId
  div= 'lastWorkflowId: ' + queueWorker.lastWorkflowId
  div= 'activityCount: ' + queueWorker.activityCount
  div= 'idleCount: ' + queueWorker.idleCount
  div= 'busyCount: ' + queueWorker.busyCount
  div= 'queueItemId: ' + queueWorker.queueItemId
  div= 'TimeStamp: ' + queueWorker.timeStamp

  div= 'Serialized Worker:' + JSON.stringify(queueWorker)

  h3= title

  table
    thead
        tr
            th Task
            th Ticks
            th WorkflowId
            th QueueItemId
            th Notes
    tbody
        each historyItem in queueWorkerHistory
            tr
                td= historyItem.task
                td= historyItem.timeStamp-queueWorker.timeStamp
                td= historyItem.workflowId
                td  <a href='/queue/history?queueItemId=#{historyItem.queueItemId}'>#{historyItem.queueItemId}</a>
                td= historyItem.notes


 


  
