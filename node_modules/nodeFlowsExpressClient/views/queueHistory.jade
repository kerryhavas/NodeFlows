
// woah!  I can use whack whack comments in jade
// also, more importantly, you can include things in the master page
block stylesheets
  link(rel='stylesheet', href='/stylesheets/chat.css')

block javascripts
 

section(class="main-content")
  h3= title
  Count:
  queuedWorkflows.count
  table
    thead
        tr
            th Type
            th QueueItemId
            th Pri
            th Ticks
            th Status
    tbody
        each queueItem in queueHistory
            tr
                td= queueItem.type
                td <a href='../../queue/history?queueItemId=#{queueItem._id}'>#{queueItem._id}</a>
                td= queueItem.priority
                td= queueItem.dateAdded
                td= queueItem.status


 


  
