
// woah!  I can use whack whack comments in jade
// also, more importantly, you can include things in the master page
block stylesheets
  link(rel='stylesheet', href='/stylesheets/chat.css')

block javascripts
 

section(class="main-content")
  h3= title
    hr
  div
    div= 'Queue History Count: ' + queueHistoryCount
    div= 'Workflow History Count: ' + workflowHistoryCount
    div= 'Node.JS Version: ' + process.version

    div= 'Memory Usage (rss): ' + process.memoryUsage().rss
    div= 'Memory Usage (heapTotal): ' + process.memoryUsage().heapTotal
    div= 'Memory Usage (heapUsed): ' + process.memoryUsage().heapUsed
    div= 'uptime: ' + process.uptime()/60 + ' minutes'
    div= 'hrtime: ' + process.hrtime()
  div
    hr
  table
      each op in stats
            tr
                td 
                    =op.subsystem
                td 
                    =op.module
                td 
                    =op.name
                td 
                    =op.count
                td 
                    =op.perHour + ' per hour'

 


  
