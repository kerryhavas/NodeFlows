
// woah!  I can use whack whack comments in jade
// also, more importantly, you can include things in the master page
block stylesheets
  link(rel='stylesheet', href='/stylesheets/chat.css')

block javascripts
 script(src='/socket.io/socket.io.js')
 script(src='/javascripts/statusFlow.js')

section(class="main-content")
  h3= title

  table
      thead
            tr
                th Id
                th Type
                th timeStamp
                th Status
                th Elapsed (ms)
        tbody
          each op in workflows
                tr
                    td
                        <a href='view?workflowid=#{op._id}'>#{op._id}</a>
                    td 
                        =op.workflowType
                    td 
                        #{new Date(op.timeStamp).getHours()} : #{new Date(op.timeStamp).getMinutes()} : #{new Date(op.timeStamp).getSeconds()}
                    td
                        =op.status
                    td
                        =op.completeTimeStamp-op.startTimeStamp
                    

div
    <a href='./select/'>Create a New Workflow</a>

 


  
