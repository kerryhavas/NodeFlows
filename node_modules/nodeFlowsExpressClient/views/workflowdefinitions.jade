
// woah!  I can use whack whack comments in jade
// also, more importantly, you can include things in the master page
block stylesheets
  link(rel='stylesheet', href='/stylesheets/chat.css')

block javascripts
 

section(class="main-content")
  h3= title
  Count:
  workflows.count
  table
      thead
            tr
                th View
                th Delete
                th Name
        tbody
          each workflow in workflows
                tr
                    td
                        <a href='viewWorkflow?workflowType=#{workflow.name}'>View</a>
                    td
                        <a href='delete?workflowid=#{workflow.name}'>Delete</a>
                    td
                        =workflow.name


 


  
